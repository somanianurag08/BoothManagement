<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">
	
	<application>
		<locale-config>
			<default-locale>en_US</default-locale>
			<supported-locale>pt_PT</supported-locale>
		</locale-config>
		
		<resource-bundle>
			<base-name>com.tatsat.boothmanagement.i18n.ComponentMessages</base-name>
			<var>componentsMsg</var>
		</resource-bundle>
		
		<resource-bundle>
			<base-name>com.tatsat.boothmanagement.i18n.MasterDataMessages</base-name>
			<var>masterDataMsg</var>
		</resource-bundle>
		
		<resource-bundle>
			<base-name>com.tatsat.boothmanagement.i18n.AdministrationMessages</base-name>
			<var>administrationMsg</var>
		</resource-bundle>
	</application>
</faces-config>